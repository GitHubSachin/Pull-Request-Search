{
    "manifestVersion": 1,
    "id": "pull-request-search",
    "version": "0.1.1",
    "name": "pull-request-search",
    "scopes": [ "vso.work", "vso.work_write" ],
    "description": "Rich pull request search experience.",
    "publisher": "ottostreifel",
    "icons": {
        "default": "img/logo.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "Sample"
    ],
    "content": {
        "details": {
            "path": "details.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://bit.ly"
        },
        "getstarted": {
            "uri": "https://bit.ly"
        },
        "learn": {
            "uri": "https://bit.ly"
        },
        "support": {
            "uri": "https://bit.ly"
        },
        "repository": {
            "uri": "https://bit.ly"
        },
        "issues": {
            "uri": "https://bit.ly"
        }
    },
    "branding": {
        "color": "rgb(220, 235, 252)",
        "theme": "light"
    },
    "files": [
        {
            "path": "img",
            "addressable": true
        },
        {
            "path": "build",
            "addressable": true
        },
        {
            "path": "index.html",
            "addressable": true
        }
    ],
    "categories": [
        "Integrate"
    ],
    "contributions": [
    ]
}
